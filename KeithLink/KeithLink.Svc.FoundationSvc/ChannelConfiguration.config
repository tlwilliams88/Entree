<channelConfiguration xmlns="http://schemas.commerceserver.net/2013/01/foundation/channelconfiguration"
                              version ="10.0.0.0"
                              useSharedCommerceContexts="false">
  <ComponentConfigurations>
    <Configuration customElementName="CreditCardEncryptionConfiguration"
                    customElementType="CommerceServer.Foundation.SequenceComponents.CreditCardEncryptionConfiguration, CommerceServer.Foundation.SequenceComponents, Version=10.0.0.0, Culture=neutral, PublicKeyToken=f5c79cb11734af7a">
      <CreditCardEncryptionConfiguration
              publicKey="registry:HKEY_LOCAL_MACHINE\SOFTWARE\CommerceServer\Encryption\Keys\bekCommerce,PublicKey"
              privateKey="registry:HKEY_LOCAL_MACHINE\SOFTWARE\CommerceServer\Encryption\Keys\bekCommerce,PrivateKey"
              deleteValues="false">
        <PropertyNames>
          <Property name="CreditCardNumber" />
          <Property name="ValidationCode" />
        </PropertyNames>
      </CreditCardEncryptionConfiguration>
    </Configuration>

    <!-- sample anti xss configurations for RIA exposed channels and sequences
    <Configuration customElementName="AntiXssConfiguration_sample_encodeSpecificProperties"
                customElementType="CommerceServer.Foundation.SequenceComponents.AntiXssElement, CommerceServer.Foundation.SequenceComponents, Version=10.0.0.0, Culture=neutral, PublicKeyToken=f5c79cb11734af7a">
      <AntiXssConfiguration_sample_encodeSpecificProperties>
        <antiXssEncoding>
          <add operation="CommerceCreateOperation" entity="Product" encoding="Html" entityProperties="DisplayName;Id"/>
          <add operation="CommerceCreateRelatedItem" entity="Variant" encoding="Html" entityProperties="*"/>
        </antiXssEncoding>
      </AntiXssConfiguration_sample_encodeSpecificProperties>
    </Configuration>
    <Configuration customElementName="AntiXssConfiguration_sample_encodeAll"
                customElementType="CommerceServer.Foundation.SequenceComponents.AntiXssElement, CommerceServer.Foundation.SequenceComponents, Version=10.0.0.0, Culture=neutral, PublicKeyToken=f5c79cb11734af7a">
      <AntiXssConfiguration_sample_encodeAll>
      <antiXssEncoding>
        <add operation="*" entity="*" encoding="Html" entityProperties="*"/>
      </antiXssEncoding>
    </AntiXssConfiguration_sample_encodeAll>
    </Configuration>
     -->
  </ComponentConfigurations>

  <Sites>
    <Site sitename="BEK_Commerce"
      metadataRepositoryPath=".\MetadataDefinitions.xml">

      <pipelines>
        <pipeline
          name="basket"
          path="pipelines\basket.pcf"
          transacted="false"
          type="OrderPipeline"
          loggingEnabled="false" />
        <pipeline
          name="total"
          path="pipelines\total.pcf"
          transacted="false"
          type="OrderPipeline"
          loggingEnabled="false" />
        <pipeline
          name="checkout"
          path="pipelines\checkout.pcf"
          transacted="true"
          type="OrderPipeline"
          loggingEnabled="false" />
        <pipeline
          name="CreditCardProcessor"
          path="pipelines\CreditCard.pcf"
          transacted="true"
          type="OrderPipeline"
          loggingEnabled="false" />
        <pipeline
          name="advertising"
          path="pipelines\Advertising.pcf"
          transacted="false"
          type="CSFPipeline"
          loggingEnabled="false" />
        <pipeline
          name="discounts"
          path="pipelines\Discounts.pcf"
          transacted="false"
          type="CSFPipeline"
          loggingEnabled="false" />
        <pipeline
          name="recordEvent"
          path="pipelines\RecordEvent.pcf"
          transacted="false"
          type="CSFPipeline"
          loggingEnabled="false" />
      </pipelines>
      <caches>
        <cache
          name="Advertising"
          type="Advertising"
          refreshInterval="850"
          retryInterval="30" />
        <cache
         name="Discounts"
         type="Discounts"
         refreshInterval="0"
         retryInterval="30" />
        <cache
         name="ShippingManagerCache"
         type="Shipping"
         loaderProgId="Commerce.ShippingMethodCache"
         refreshInterval="0"
         retryInterval="30" />
        <cache name="PaymentMethodCache" type="Payment"/>
        <cache name="OrdersConfigurationCache" type="OrdersConfiguration" />
      </caches>
      <messageManager>
        <cultures
           default="en-US"
           baseName="CommerceMessageManager"
           assembly="CommerceMessageManager">
          <culture id="en-US"/>
          <culture id="fr-FR"/>
          <culture id="ja-JP"/>
          <culture id="de-DE"/>
        </cultures>
        <resources>
          <resource id="pur_badsku"/>
          <resource id="pur_badplacedprice"/>
          <resource id="pur_discount_changed"/>
          <resource id="pur_discount_removed"/>
          <resource id="pur_noitems"/>
          <resource id="pur_badshipping"/>
          <resource id="pur_badhandling"/>
          <resource id="pur_badtax"/>
          <resource id="pur_badcc"/>
          <resource id="pur_badpayment"/>
          <resource id="pur_badverify"/>
          <resource id="pur_out_of_stock"/>
          <resource id="unknown_shipping_method"/>
        </resources>
      </messageManager>
      <catalog  enableInventorySystem="true">
        <catalogSets
           defaultAuthenticated="Registered User Default CatalogSet"
           defaultAnonymous="Anonymous User Default CatalogSet" />
        <cache
           enable="true"
           schemaTimeout="20"
           itemInformationCacheTimeout ="20"
           itemHierarchyCacheTimeout ="20"
           itemRelationshipsCacheTimeout = "20"
           itemAssociationsCacheTimeout="20"
           catalogCollectionCacheTimeout="20" />
      </catalog>

      <commerceEvent>
        <add className="CommerceServer.Core.Runtime.AddItemToBasketEvent" id="AddItemToBasket" />
        <add className="CommerceServer.Core.Runtime.SubmitOrderEvent" id="SubmitOrder" loggingEnabled="true" />
      </commerceEvent>

      <contentSelection>
        <add name="Advertising"
          cacheName="Advertising"
          selectionPipeline="advertising"
          eventPipeline="recordEvent"
          preloadCache="false"
          redirectUrl="~home/_layouts/CommerceServer/AdsRedir.ashx" />
        <add name="Discounts"
          cacheName="Discounts"
          selectionPipeline="discounts"
          eventPipeline="recordEvent"
          preloadCache="false"
          redirectUrl="~home/_layouts/CommerceServer/AdsRedir.ashx" />
      </contentSelection >

      <orders honorStatus="true" newOrderStatus="NewOrder">
        <addressMap name="Address">
          <property from="GeneralInfo.address_id" to="OrderAddressId" />
          <property from="GeneralInfo.first_name" to="FirstName" />
          <property from="GeneralInfo.last_name" to="LastName" />
          <property from="GeneralInfo.address_line1" to="Line1" />
          <property from="GeneralInfo.address_line2" to="Line2" />
          <property from="GeneralInfo.city" to="City" />
          <property from="GeneralInfo.region_code" to="RegionCode" />
          <property from="GeneralInfo.postal_code" to="PostalCode" />
          <property from="GeneralInfo.country_code" to="CountryCode" />
          <property from="GeneralInfo.country_name" to="CountryName" />
          <property from="GeneralInfo.region_name" to="State" />
          <property from="GeneralInfo.tel_number" to="DaytimePhoneNumber" />
        </addressMap>
