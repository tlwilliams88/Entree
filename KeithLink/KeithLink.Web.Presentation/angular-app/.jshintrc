//Removed "eqeqeq": true, which does this This options prohibits the use of == and != in favor of === and !==. The former try to coerce values before comparing them which can lead to some unexpected results. The latter don't do any coercion so they are generally safer.

{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "curly": true,
  "immed": true,
  "indent": 2,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "predef": [ "jQuery", "c3", "cordova", "moment", "jstz" ],
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "-W041": false,
  "globals": {
    "angular": false
  },
  "debug": true
}
